{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": ["plugin:react/recommended", "standard-with-typescript"],
  "overrides": [],
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": ["error", "always"],
    "@typescript-eslint/semi": ["error", "always"]
  }
}
